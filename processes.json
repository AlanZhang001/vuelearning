{
  "apps": [{
    "name": "vue",
    "cwd": "./",
    "script": "./server/index.js",
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "error_file": "./logs",
    "out_file": "./logs/node-app.stdout.log",
    "pid_file": "./logs/node-geo-api.pid",
    "instances": 6,
    "min_uptime": "200s",
    "max_restarts": 10,
    "max_memory_restart": "150M",
    "cron_restart": "1 0 * * *",
    "watch": false,
    "merge_logs": true,
    "exec_interpreter": "node",
    "exec_mode": "fork",
    "autorestart": true,
    "vizion": false
  }]
}